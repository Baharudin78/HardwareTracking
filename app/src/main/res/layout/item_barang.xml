<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardBackgroundColor="@color/white"
    android:layout_marginTop="10dp"
    app:cardCornerRadius="8dp"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="3373j83n434xmc"
            android:fontFamily="@font/latto_reguler"
            android:layout_marginStart="15dp"
            android:id="@+id/tvQrCode"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/black"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="3373j83n434xmc"
            android:fontFamily="@font/latto_reguler"
            android:id="@+id/tvNamaBarang"
            android:layout_marginStart="15dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/tvQrCode"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/black"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="3373j83n434xmc"
            android:layout_marginStart="15dp"
            android:fontFamily="@font/latto_reguler"
            android:layout_marginTop="8dp"
            android:id="@+id/tvNamaPemegang"
            android:layout_marginBottom="5dp"
            app:layout_constraintTop_toBottomOf="@+id/tvNamaBarang"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/black"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="3373j83n434xmc"
            android:fontFamily="@font/latto_reguler"
            android:layout_marginStart="15dp"
            android:layout_marginTop="8dp"
            android:id="@+id/tvCurrentLoc"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="5dp"
            app:layout_constraintTop_toBottomOf="@+id/tvNamaPemegang"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/black"/>

    </androidx.constraintlayout.widget.ConstraintLayout>



</com.google.android.material.card.MaterialCardView>